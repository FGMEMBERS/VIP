      				<x-offset-m archive="y">-1.84 </x-offset-m> <!--Left/Right-->
      				<y-offset-m archive="y"> 0.99 </y-offset-m> <!--Up/Down-->
      				<z-offset-m archive="y"> 0.23 </z-offset-m> <!--Fdw/Back-->
    			</config>
  		</view>
  
  		<view n="102">
   			<name>Tail view</name>
   			<type>lookfrom</type>
   			<internal type="bool">false</internal>
